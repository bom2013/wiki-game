{
  "background": {
    "scripts": ["jquery-3.5.0.min.js", "js/wiki_api_function.js", "background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["jquery-3.5.0.min.js", "js/wiki_api_function.js", "content.js"],
    "matches": ["https://he.wikipedia.org/*"]
  }],
  "icons": {
    "16": "images/wiki-logo.svg",
    "32": "images/wiki-logo.svg",
    "48": "images/wiki-logo.svg",
    "128": "images/wiki-logo.svg"
  },
  "author": {
    "name": "Noam Ben Shlomo",
    "github": "https://github.com/bom2013"
  },
  "manifest_version": 2,
  "name": "The Wikipedia Game",
  "permissions": ["https://he.wikipedia.org/", "tabs", "storage"],
  "version": "1.0"
}
