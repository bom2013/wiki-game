{
  "background": {
    "scripts": ["jquery-3.5.0.min.js", "js/wiki_api_function.js", "background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["jquery-3.5.0.min.js", "js/wiki_api_function.js", "content.js"],
    "matches": ["https://he.wikipedia.org/*"]
  }],
  "manifest_version": 2,
  "name": "The Wikipedia Game",
  "permissions": ["https://he.wikipedia.org/", "tabs", "storage"],
  "version": "1.0"
}
